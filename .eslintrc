{
    "extends": [
        "prettier",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier",
        "jsx-a11y"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "react/prop-types": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "mocha": true
    },
    "parserOptions": {
        "ecmaVersion": 2020
    }
}
